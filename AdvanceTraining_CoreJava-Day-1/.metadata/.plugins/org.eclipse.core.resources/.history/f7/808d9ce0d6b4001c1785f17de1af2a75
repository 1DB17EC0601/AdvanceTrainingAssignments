package com;


import java.util.Scanner;
public class TextBook {
	
	  
		
			
			Scanner sc = new Scanner(System.in);
			
			System.out.println("Enter the bookname");
			String bName=sc.next();
			
			System.out.println("Enter the bookprice");
			double bprice=sc.nextDouble();
			
			public static void main(String[] args) {
			Book b = new Book();
			b.setBookTitle(bName);
			b.setBookPrice(bprice);
			
			System.out.println("The Book Details");
			
			System.out.println( "BookName: "+b.getBookTitle()+" BookPrice ");
			System.out.println( "BookPrice: "+b.getBookTitle());
